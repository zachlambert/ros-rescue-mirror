<launch>
    <param
        name="robot_description"
        command="cat $(find rescue_control)/model/rescue.urdf"
    />

    <rosparam
        file="$(find rescue_gazebo)/config/gazebo_ros_control.yaml"
        command="load"
    />
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg
            name="world_name"
            value="$(find rescue_gazebo)/worlds/rescue.world"
        />
    </include>
    <node
        name="spawn_model"
        pkg="gazebo_ros"
        type="spawn_model"
        args="-file $(find rescue_control)/model/rescue.urdf -urdf -x 0 -y 0 -z 0.5 -model rescue"
    />
</launch>
