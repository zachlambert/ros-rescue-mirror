<launch>
    <!-- Include the move_group launch file generated by the moveit setup
    assistant, which loads relevant parameters and starts the move group
    node, which provides services and action servers for planning and
    executing trajectories -->
    <include file="$(find rescue_control_moveit)/launch/move_group.launch"/>

    <node
        name="inverse_kinematics"
        pkg="rescue_control"
        type="inverse_kinematics"
        output="screen"
    />
</launch>
