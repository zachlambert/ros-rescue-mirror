<launch>
    <param
        name="robot_description"
        command="xacro $(find arbie_description)/model/arbie.urdf.xacro"
    />
    <rosparam
        file="$(find arbie_hardware)/config/usb.yaml"
        command="load"
    />

    <node name="odrives" pkg="arbie_hardware" type="odrives" output="screen"/>
    <node
        name="hardware"
        pkg="arbie_hardware"
        type="hardware"
        output="screen"
    />
</launch>
